    "tw-animate-css": "1.4.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1",
    "@cloudflare/workers-types": "^4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "8.5.6",
